      "source": [
        "itr=100\r\n",
        "learningrate=0.01\r\n",
        "x=1\r\n",
        "y=-1\r\n",
        "def diffx(x):\r\n",
        "\treturn 2*x\t\r\n",
        "def diffy(y):\r\n",
        "\treturn 2*y\r\n",
        "for i in range(itr):\r\n",
        "    deltax=-1*learningrate*diffx(x)\r\n",
        "    deltay=-1*learningrate*diffy(y)\r\n",
        "    x=x+deltax\r\n",
        "    y=y+deltay\r\n",
        "print(\"Optimal value of x:\",x,\"\\nOptimal value of y:\",y)"
      ],
